{
  "name": "ignition",
  "main": "dist/app.js",
  "version": "0.1.0",
  "description": "Get off the ground with React, Redux, Apollo and Webpack.",
  "productName": "Ignition",
  "homepage": "https://github.com/saeris/ignition#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/saeris/ignition.git"
  },
  "author": "Drake Costa <drake@saeris.io> (http://saeris.io/)",
  "keywords": [
    "react",
    "redux",
    "apollo",
    "graphql",
    "webpack"
  ],
  "bugs": {
    "url": "https://github.com/saeris/ignition/issues"
  },
  "scripts": {
    "clean": "npm cache clean && del-cli node_modules",
    "clean:dist": "del-cli dist",
    "preclean:install": "npm run clean",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --progress --profile",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "cross-env NODE_ENV=production npm run webpack -- --progress --profile",
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "cross-env NODE_ENV=development npm run webpack-dev-server -- --progress --profile --watch --colors",
    "server:prod": "http-server dist --cors",
    "webpack": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack",
    "webpack-dev-server": "cross-env BABEL_ENV=node ./node_modules/.bin/webpack-dev-server"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "font-awesome": "^4.6.3",
    "history": "^4.6.3",
    "include-media": "^1.4.9",
    "jquery": "^3.0.0",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "prop-types": "^15.5.10",
    "react-apollo": "^1.4.3",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.2",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-css-modules": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-commonjs-es2015-modules": "^3.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.18.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-native": "^2.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "del-cli": "^1.1.0",
    "eslint": "^4.2.0",
    "eslint-plugin-compat": "^1.0.1",
    "eslint-plugin-graphql": "^1.1.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "graphql-tag": "^2.4.2",
    "html-webpack-plugin": "^2.29.0",
    "ignore-loader": "^0.1.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-load-config": "^1.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.0.4",
    "postcss-scss": "^1.0.2",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "postcss": {
    "plugins": {
      "postcss-cssnext": {
        "browsers": [
          "last 2 versions"
        ]
      },
      "postcss-flexbugs-fixes": {}
    }
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      "last 4 versions"
    ]
  }
}
